<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitness.view.Login.LogInController">
   <children>
      <ImageView fitHeight="305.0" fitWidth="500.0" layoutY="-2.0">
         <image>
            <Image url="@../image/健身房背景图.jpg" />
         </image>
      </ImageView>
      <Button layoutX="75.0" layoutY="38.0" mnemonicParsing="false" opacity="0.4" prefHeight="220.0" prefWidth="350.0" style="-fx-border-color: null;">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TextField fx:id="userId" layoutX="134.0" layoutY="74.0" prefHeight="30.0" prefWidth="233.0" promptText="用户名" />
      <PasswordField fx:id="password" layoutX="134.0" layoutY="133.0" prefHeight="30.0" prefWidth="233.0" promptText="密码" />
      <Button fx:id="login_button" layoutX="134.0" layoutY="192.0" mnemonicParsing="false" onAction="#login_button_action" onMouseEntered="#login_button_OnMouseEntered" prefHeight="30.0" prefWidth="233.0" style="-fx-background-color: rgb(0,153,204);" text="登录" textFill="WHITE" />
      <ImageView fx:id="close_button" fitHeight="16.0" fitWidth="16.0" layoutX="477.0" layoutY="5.0" onMouseClicked="#close_button_action" onMouseEntered="#close_button_OnMouseEntered" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/关闭（白）.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
