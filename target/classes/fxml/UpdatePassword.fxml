<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="350.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fitness.view.update_password.UpdatePasswordController">
   <children>
      <ImageView fitHeight="351.0" fitWidth="360.0">
         <image>
            <Image url="@../image/健身房背景图.jpg" />
         </image>
      </ImageView>
      <Button layoutY="40.0" mnemonicParsing="false" opacity="0.4" prefHeight="311.0" prefWidth="360.0" style="-fx-border-color: null;">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label prefHeight="40.0" prefWidth="360.0" style="-fx-background-color: rgb(0,199,197);" text="   更改密码" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font></Label>
      <TextField fx:id="email" layoutX="64.0" layoutY="75.0" prefHeight="30.0" prefWidth="233.0" promptText="请输入验证邮箱" />
      <Button fx:id="ok_button" layoutX="64.0" layoutY="285.0" mnemonicParsing="false" onAction="#ok_button_action" onMouseEntered="#ok_button_OnMouseEntered" prefHeight="30.0" prefWidth="233.0" style="-fx-background-color: rgb(0,199,197);" text="确定" textFill="WHITE" />
      <ImageView fx:id="close_button" fitHeight="13.0" fitWidth="13.0" layoutX="338.0" layoutY="6.0" onMouseClicked="#close_button_action" onMouseEntered="#close_button_OnMouseEntered" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/关闭（白）.png" />
         </image>
      </ImageView>
      <PasswordField fx:id="pswd_again" layoutX="64.0" layoutY="175.0" prefHeight="30.0" prefWidth="233.0" promptText="请再次输入新密码" />
      <PasswordField fx:id="newpswd" layoutX="64.0" layoutY="125.0" prefHeight="30.0" prefWidth="233.0" promptText="请输入新密码" />
      <Button fx:id="send_code_button" layoutX="196.0" layoutY="225.0" mnemonicParsing="false" onAction="#send_code_button_action" onMouseEntered="#send_code_button_OnMouseEntered" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: rgb(40,70,90);" text="发送验证码" textFill="WHITE" />
      <TextField fx:id="code" layoutX="63.0" layoutY="225.0" prefHeight="30.0" prefWidth="100.0" promptText="验证码" />
   </children>
</AnchorPane>
